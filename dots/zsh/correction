setopt correct_all

alias man='nocorrect man'
alias mv='nocorrect mv'
alias mysql='nocorrect mysql'
alias mkdir='nocorrect mkdir'
alias gist='nocorrect gist'
alias heroku='nocorrect heroku'
alias ebuild='nocorrect ebuild'
alias hpodder='nocorrect hpodder'
alias npm='nocorrect npm'
alias bundler='nocorrect bundler'
alias bundle='nocorrect bundle'
alias pry='nocorrect pry'
alias tree='nocorrect tree'
alias cheat='nocorrect cheat'
alias rspec='nocorrect rspec'
alias wget='nocorrect wget'

# keep this here as a fix for rake
alias rake='noglob rake'

alias rvm='nocorrect rvm'
alias gulp='nocorrect gulp'
alias esformatter='nocorrect esformatter'

# do not autocorrect some of the git commands
alias git st='nocorrect git st'
alias git='nocorrect git'

alias yarn='nocorrect yarn'
alias npm='nocorrect npm'
